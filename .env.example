# Cesium Ion Access Token
# Get free token at https://ion.cesium.com/
NEXT_PUBLIC_CESIUM_ION_TOKEN=your_cesium_ion_token_here

# Supabase Configuration
# Get these from your Supabase project settings (https://supabase.com/dashboard/project/_/settings/api)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database (Legacy - still needed for Prisma if using both)
# For development (SQLite)
DATABASE_URL="file:./dev.db"

# For production (PostgreSQL - Supabase)
# DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.your-project.supabase.co:5432/postgres"

# Admin Password (for moderation)
ADMIN_PASSWORD="your_secure_admin_password_here"
